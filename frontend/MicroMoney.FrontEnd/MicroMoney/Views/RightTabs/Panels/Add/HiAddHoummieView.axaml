<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="clr-namespace:MicroMoney.ViewModels.RightTabs.Panels.Add"
             x:DataType="vm:HiAddHoummieViewModel"
             x:Class="MicroMoney.Views.RightTabs.Panels.Add.HiAddHoummieView">
   <Border HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
    <StackPanel Orientation="Vertical">
      <Button Command="{Binding AddKingdomCommand}" Content="Добавить Спадарства" Margin="20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
      <Button Command="{Binding AddCommand}" Content="Добавить человека" Margin="20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </StackPanel>
  </Border>
</UserControl>
